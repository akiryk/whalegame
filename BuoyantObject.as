class BuoyantObject extends MovingObject{	private var buoyancy:Number;	private var m:Number; // mass		public function BuoyantObject(xpos:Number, ypos:Number, xvel:Number, yvel:Number, xaccel:Number, yaccel:Number, mc:MovieClip, m:Number) {		super(xpos, ypos, xvel, yvel, xaccel, yaccel, mc);		if (yaccel == undefined) {			this.buoyancy = 0;		} else {			this.buoyancy = yaccel;		}		if (m == undefined ){			this.m = 1;		} else {			this.m = m;		}	}}