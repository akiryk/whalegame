class Trig {		public function Trig() {		//	}		public static function getDistByV(pos1:Vector, pos2:Vector):Number {		var dx:Number = Math.abs(pos1.x - pos2.x);		var dy:Number = Math.abs(pos1.y - pos2.y);		return Math.sqrt(dx * dx + dy * dy); // return length of hypotenuse	}		public static function getDist(x1:Number, y1:Number, x2:Number, y2:Number):Number {		var dx:Number = Math.abs(x1 - x2);		var dy:Number = Math.abs(y2 - y2);		return Math.sqrt(dx * dx + dy * dy); // return length of hypotenuse	}}