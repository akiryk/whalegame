class ListOfObjects {	private var objectsToManage:Array;		public function ListOfObjects() {		objectsToManage = new Array();	}			public function addObject(o:Object):Void {		objectsToManage.push(o);	}		public function removeObject(o:Object):Void {		for (var i:Number = 0; i < objectsToManage.length; i++){			if (o == objectsToManage[i]){				objectsToManage.splice(i,1);				break;			}		}	}		public function getLength():Number {		return objectsToManage.length;	}		public function getIndex(i:Number):Object {		return objectsToManage[i];	}} 