class MovingObject {	private var _mc:MovieClip;	private var pos:Vector;	private var vel:Vector;	private var accel:Vector;		public function MovingObject(xpos:Number, ypos:Number, xvel:Number, yvel:Number, xaccel:Number, yaccel:Number, mc:MovieClip) {		this.pos = new Vector(xpos, ypos);		this.vel = new Vector (xvel, yvel);		this.accel = new Vector (xaccel, yaccel);		this._mc = mc;	}}